<%- include ('../../partials/header.ejs')%>
<body>
    <%- include ('../../partials/navbar.ejs')%>
    <div class="container">
        <hr>
        <h1>Usuários</h1>
        <a class="btn btn-primary" href="/admin/users/create">Criar novo Usuário</a>
        <hr>
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th>ID</th>
                    <th>Email</th>
                </tr>
            </thead>
            <tbody>
                <% users.forEach(user => { %>
                    <tr>
                        <th><%= user.id %></th>
                        <th><%= user.email %></th>
                        </th>
                    </tr>
                <% }) %>
            </tbody>
        </table>
    </div>

</body>
<%- include ('../../partials/footer.ejs')%>