<body>
    <%- include ('../../partials/navbar.ejs')%>
    

    <div class="container">
        <hr>
        <% result.articles.rows.forEach(article => { %>
            <hr> 
            <div class="card">
                <div class="card-header">
                    <%= article.title %>
                </div>
                <div class="card-body">
                    <a href="/<%= article.slug %>" class="btn btn-primary">Ler Artigo</a>
                </div>
                
            </div>
            <br>

        <% }) %>
    </div>

</body>

